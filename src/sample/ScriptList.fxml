<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/8.0.201" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ScriptListController">
   <children>
  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="600.0" prefWidth="720.0" style="-fx-background-color: #42a4f5;">
         <children>
            <ListView fx:id="ScriptListView" layoutX="133.0" layoutY="52.0" prefHeight="150.0" prefWidth="455.0" />
            <Button fx:id="ViewScriptBut" layoutX="133.0" layoutY="228.0" mnemonicParsing="false" onAction="#viewScript" text="View" />
            <TextArea fx:id="viewScript" layoutX="133.0" layoutY="278.0" prefHeight="305.0" prefWidth="455.0" />
            <ComboBox fx:id="importOrCreate" layoutX="438.0" layoutY="228.0" prefWidth="150.0" promptText="Import or Create" />
            <Button layoutX="196.0" layoutY="228.0" mnemonicParsing="false" text="Delete" />
            <Button fx:id="logoutBut" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#logout"
                    text="Logout" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
